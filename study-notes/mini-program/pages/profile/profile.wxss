/* pages/profile/profile.wxss */
.login-wrapper {
    height: 400rpx;
    display: flex;
    position: relative;
    justify-content: center;
    align-items: center;
}

.login-wrapper image {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
}

.login-wrapper .login {
    color: #1E4826;
    background-color: #fff;
    border-radius: 50rpx;
    font-size: 50rpx;
    height: 100rpx;
    line-height: 100rpx;
    width: 350rpx;
    text-align: center;
}

.version {
    position: absolute;
    bottom: 30rpx;
    color: #C9C9C9;
    font-size: 20rpx;
    width: 100%;
    text-align: center;
}